			}
  v[13]=p->cal("Pub_Debt",0);
  v[14]=p->cal("Pub_Debt",4);
  			if(v[14]!=0)
			v[20]=(v[13]-v[14])/v[14];// crescimento divida_Pub
			else
			{
			v[20]=0;
			}
  v[21]=v[13]/v[9];//Pub_Debt/Anual_GDP
  
  if(v[21]<0.3)
				v[12]=v[16]-v[17];
			else
			{
				if(v[21]>=0.3&&v[21]<=0.4)
				{
					if(v[20]<v[10])
						v[12]=v[16]-v[17];
					else
						v[12]=v[16];
				}
				else
					v[12]=v[16]+v[17];
			}
	}	
	else
		v[12]=v[16];

res=min(max(-0.005,v[12]),0.03);
goto end;
}


