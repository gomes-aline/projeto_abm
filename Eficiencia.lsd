*/

v[16]=p->cal("Superavit_rate_target",1);
v[17]=p->cal("var_supertargetP",0);
v[18]= fmod((double) t,4);

  if(v[18]==0)
  {
  
  v[0]=p->cal("Q_GDP",7);
  v[1]=p->cal("Q_GDP",6);
  v[2]=p->cal("Q_GDP",5);
  v[3]=p->cal("Q_GDP",4);
  v[4]=(v[0]+v[1]+v[2]+v[3]);
  v[5]=p->cal("Q_GDP",3);
  v[6]=p->cal("Q_GDP",2);
  v[7]=p->cal("Q_GDP",1);
  v[8]=p->cal("Q_GDP",0);
  v[9]=(v[5]+v[6]+v[7]+v[8]);
  v[10]=(v[9]-v[4])/v[4];//variação do valor agregado anual
  
  			if(v[4]!=0)
